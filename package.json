{
  "name": "netbots",
  "version": "0.0.1",
  "description": "NetBots is a robot programming game",
  "main": "main.js",
  "scripts": {
    "start": "node dist/server.js",
    "start-dev": "node out/server.js",
    "dev": "rm -rf out/ && npm run scss-build-dev && tsc-watch -p tsconfig.json --onSuccess \"concurrently -p \"[{name}]\" -n \"LINT,SCSS,SERVER\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" --kill-others-on-fail \"npm:lint\" \"npm:scss-watch\" \"npm:start-dev\"\"",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "scss-watch": "node-sass src/public/scss -o out/public/css --watch --source-map true",
    "scss-build": "node-sass src/public/scss -o dist/public/css --output-style compressed",
    "scss-build-dev": "node-sass src/public/scss -o out/public/css --source-map true",
    "ts-watch": "rm -rf out/ && tsc -w",
    "ts-build": "rm -rf dist/ && tsc --outDir dist/ --sourceMap false --incremental false",
    "build": "npm run lint && npm run ts-build && npm run scss-build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://eoic@bitbucket.org/eoic/web-bots-it.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/eoic/web-bots-it#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "connect-mongodb-session": "^2.0.3",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^8.1.0",
    "express": "^4.16.3",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "express-sitemap": "^1.8.0",
    "express-validator": "^5.3.0",
    "handlebars": "^4.0.14",
    "jdenticon": "^2.1.1",
    "memorystore": "^1.6.0",
    "mocha": "^5.2.0",
    "mongoose": "^5.3.1",
    "morgan": "^1.9.1",
    "nodemailer": "^5.0.0",
    "object-sizeof": "^1.3.0",
    "uuid": "^3.3.2",
    "vm2": "^3.6.4",
    "ws": "^6.1.0"
  },
  "devDependencies": {
    "@sendgrid/mail": "^6.4.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/express-handlebars": "0.0.33",
    "@types/express-session": "^1.15.14",
    "@types/mongoose": "^5.5.17",
    "@types/morgan": "^1.7.37",
    "@types/uuid": "^3.4.5",
    "@types/ws": "^6.0.3",
    "concurrently": "^4.1.2",
    "node-sass": "^4.12.0",
    "tsc-watch": "^3.0.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.0",
    "utf-8-validate": "^5.0.1"
  }
}
